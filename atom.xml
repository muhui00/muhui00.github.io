  <entry>
    <title>Protobuf数据序列化</title>
    <link href="http://example.com/2022/04/15/Protobuf%E6%95%B0%E6%8D%AE%E5%BA%8F%E5%88%97%E5%8C%96/"/>
    <id>http://example.com/2022/04/15/Protobuf%E6%95%B0%E6%8D%AE%E5%BA%8F%E5%88%97%E5%8C%96/</id>
